mainclass IfTest {
    public static void main ( ) {

        if ( 1 != 1 ) {
            println(1);
        }

        if ( 1 != 1.0 ) {
            println(1);
        }

        if ( 1.0 != 1 ) {
            println(1);
        }

        if ( 1.0 != 1.0 ) {
            println(1);
        }

        if ( 3.2 <= 0 ) {
            println(1);
        }

        if ( 9 > 10 ) {
            println(1);
        }

        if ( 10 / 3.0 >= 4 ) {
            println(1);
        }
    }
}