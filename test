mainclass IntFloatMixups {
public static void main ( )
{
    int i;
    float f;

    i = 3.3;
    if ( i != 3 ) {
        println(1);
    }

    if ( (i = 5) != 5 ) {
        println(1);
    }

    if ( i != 5.0 ) {
        println(1);
    }

    if ( 1.0 != 1 ) {
        println(1);
    }

    if ( 1 != (i = 1.98) ) {
        println(1);
    }

    if ( 3 * 10 / 3 != 10 ) {
        println(1);
    }

    if ( 10 / 3 * 3 != 9 ) {
        println(1);
    }

    if ( -10 / 11 != 0 ) {
        println(1);
    }

    if ( 1 + 9 * 0 != 1 ) {
        println(1);
    }

    if ( 9 / 2.0 != 4.5 ) {
        println(1);
    }

    float sum;
    f = 4;
    for (i=0; i<=f; i=i+1) {
        sum = sum + i;
    }

    if ( sum != 10 ) {
        println(1);
    }
}
}